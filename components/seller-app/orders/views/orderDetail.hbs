 <section class="breadcrumb-section set-bg" data-setbg="/sellerRole/img/breadcrumb.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb__text">
                        <h2>Orders Detail</h2>
                        <div class="breadcrumb__option">
                            <a href="/">Home</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</section>
<section class="shoping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="shoping__cart__table">
                        <table>
                            <thead>
                                <tr>
                                    <th>Order_id</th>
                                    <th>Product_id</th>
                                    <th style="width: calc(100% / 3);">Product_name</th>
                                    <th>Price</th>
                                    <th>Quantity</th>
                                </tr>
                            </thead>
                            <tbody>
                                    {{#each product1 as |product|}}
                                            <tr>
                                                <td>
                                                   {{product.[0].order_id}}
                                                </td>
                                                <td >
                                                    {{product.[0].product_id}}
                                                </td>
                                                <td >
                                                   {{!-- {{product.[0].'product.product_name'}}  --}}
                                                </td>
                                                <td>
                                                    {{!-- {{'product.price'}} --}}
                                                </td>
                                                <td>
                                                    {{product.[0].quantity}}
                                                </td>
                                             
                                                
                                              
                                            </tr>
                                    {{/each}}
                            
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            
        </div>
    </section>
   
