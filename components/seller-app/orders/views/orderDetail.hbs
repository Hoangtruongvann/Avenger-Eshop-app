    <div class="container-fluid mt--6">
    <div style="margin-top: 101px" class="row justify-content-center">
        <div class=" col ">
            <div class="card">
                <div class="card-header bg-transparent">
                    <h3 class="mb-0">Chi tiết đơn hàng</h3>
                </div>
                <div class="card-body">
                    <div class="container">

                        <div class="row icon-examples product-tittle">
                            {{#if detail}}
                            <div class="col-2">
                                <h5>Hình ảnh</h5>
                            </div>
                            <div class="col-2">
                                <h5>Tên sản phẩm</h5>
                            </div>
                            <div class="col-2">
                                <h5>Tên loại</h5>
                            </div>
                            <div class="col-2">
                                <h5>Tên hãng</h5>
                            </div>
                            <div class="col-2">
                                <h5>Đơn giá</h5>
                            </div>
                            <div class="col-1">
                                <h5>Số lượng</h5>
                            </div>

                        </div>


                        <div class="row icon-examples" style="margin: -5px -61px;">
                            {{#each detail}}
                            <div class="col-lg-12">

                                <button type="button" class="btn-icon-clipboard" data-clipboard-text="active-40">
                                    <div class="row">
                                        <div class="col-2 ">
                                            <img src="{{'product.images.image_link'}}" alt="">
                                        </div>
                                        <div class="col-2 ">
                                            {{'product.product_name'}}
                                        </div>
                                        <div class="col-2 ">
                                            {{'product.category.category_name'}}
                                        </div>
                                        <div class="col-2 ">
                                            {{'product.brand.brand_name'}}
                                        </div>
                                        <div class="col-2 ">
                                             {{'product.price'}}
                                        </div>
                                        <div class="col-2 ">
                                            {{quantity}}
                                        </div>
                                        
                                    </div>
                                   
                                </button>
                                
                            </div>
                            
                            {{/each}}

                        </div>
                    </div>
                </div>
                {{else}}
                <h1>Thêm don hang và bắt đầu kinh doanh ngay bây giờ!</h1>
                {{/if}}
         

            <div class="product__pagination" style="margin:auto">
                {{#if first.state}}
                <a style="display:none;" href="/seller/orders/detail/{{first.order_id}}?page={{first.page}}" ><i class="fa fa-long-arrow-left" style="display:none;"></i></a>
                        {{else}}
                <a href="/seller/orders/detail/{{first.order_id}}?page={{first.page}}"><i class="fa fa-long-arrow-left"></i></a>
                {{/if}}

                {{#each page }}
                    {{#if this.active}}
                        <a href="/seller/orders/detail/{{this.order_id}}?page={{this.num}}" style="background-color: green; color:white;">{{this.num}}</a>
                        {{else}}
                        <a href="/seller/orders/detail/{{this.order_id}}?page={{this.num}}">{{this.num}}</a>

                    {{/if}}
                {{/each}}

                 {{#if last.state}}
                            <a style="display:none;" href="/seller/orders/detail/{{last.order_id}}?page={{last.page}} ><i class="fa fa-long-arrow-right" ></i></a>
                            {{else}}
                            <a href="/seller/orders/detail/{{last.order_id}}?page={{last.page}}"><i class="fa fa-long-arrow-right"></i></a>
                {{/if}}
            </div>
        </div>
        </div>
    </div>
</div>
