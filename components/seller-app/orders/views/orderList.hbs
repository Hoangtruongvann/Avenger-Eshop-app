 <section class="breadcrumb-section set-bg" data-setbg="/sellerRole/img/breadcrumb.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb__text">
                        <h2>Orders</h2>
                        <div class="breadcrumb__option">
                            <a href="/">Home</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</section>
<section class="shoping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="shoping__cart__table">
                        <table>
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Date</th>
                                    <th>Total price</th>
                                    <th>Status</th>
                                    <th>Company</th>
                                    <th>Shop</th>
                                   
                                   
                                </tr>
                            </thead>
                            <tbody>
                                    {{#each orders}}
                                            <tr>
                                                <td >
                                                    {{order_id}}
                                                </td>
                                                <td >
                                                    {{order_date}}
                                                </td>
                                                <td>
                                                    {{order_status}}
                                                </td>
                                                <td>
                                                    {{order_total_price}}
                                                </td>
                                                <td>
                                                    {{'shipp_com.company_name'}}
                                                </td>
                                                <td>
                                                    {{'shop.shop_name'}}
                                                </td>
                                              
                                            </tr>
                                    {{/each}}
                            
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            
        </div>
    </section>

